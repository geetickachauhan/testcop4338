#include <stdio.h>
#include <string.h>

void C(FILE *f){

	char line[81];
	char* words[81];
	while(fgets(line, sizeof(line), f))
	{
		size_t len = strlen(line);
		line[--len] = '\0';
		char* word;
		int i = 0;
		word = strtok(line, "\n ");
		while((word != NULL)){
			words[i] = word;
			word = strtok(NULL, " ");
			i++;
		}
		for(i = i-1; i >= 0; i--){
			printf("%s ", words[i]);
		}
		printf("\n");
	}
}

